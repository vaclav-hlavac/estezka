                "operationId": "10c02a951e844462c465d0e576b8c942",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nickname",
                                    "name",
                                    "surname",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "nickname": {
                                        "type": "string",
                                        "example": "scoutjoe"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Joe"
                                    },
                                    "surname": {
                                        "type": "string",
                                        "example": "Doe"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "joe@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "hunter2"
                                    },
                                    "new_troop": {
                                        "properties": {
                                            "name": {
                                                "type": "string",
                                                "example": "Falcons"
                                            }
                                        },
                                        "type": "object"
                                    },
                                    "invite_code": {
                                        "type": "string",
                                        "example": "a1b2c3d4"
